t\t\t<el-input v-model=\"dataForm.id\" :disabled=\"!operation\" auto-complete=\"off\" ></el-input>\r\n\t\t\t</el-form-item>\r\n\t\t\t<el-form-item label=\"人数(>=)\" prop=\"minnum\" >\r\n\t\t\t\t<el-input v-model.number=\"dataForm.minnum\" auto-complete=\"off\"  maxlength=\"8\"></el-input>\r\n\t\t\t</el-form-item>\r\n\t\t\t<el-form-item label=\"人数(<=)\" prop=\"maxnum\">\r\n\t\t\t\t<el-input v-model.number=\"dataForm.maxnum\" auto-complete=\"off\" maxlength=\"8\"></el-input>\r\n\t\t\t</el-form-item>\r\n      <el-form-item label=\"奖励比率(%)\" prop=\"rate\" maxlength=\"6\">\r\n        <el-input v-model=\"dataForm.rate\" auto-complete=\"off\"></el-input>\r\n      </el-form-item>\r\n\t\t</el-form>\r\n\t\t<div slot=\"footer\" class=\"dialog-footer\">\r\n\t\t\t<el-button :size=\"size\" @click.native=\"dialogVisible = false\">{{$t('action.cancel')}}</el-button>\r\n\t\t\t<el-button :size=\"size\" type=\"primary\" @click.native=\"submitForm\" :loading=\"editLoading\">{{$t('action.submit')}}</el-button>\r\n\t\t</div>\r\n\t</el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport PopupTreeInput from \"@/components/PopupTreeInput\"\r\nimport KtTable from \"@/views/Core/KtTable\"\r\nimport KtButton from \"@/views/Core/KtButton\"\r\nimport TableColumnFilterDialog from \"@/views/Core/TableColumnFilterDialog\"\r\nimport { format } from \"@/utils/datetime\"\r\nexport default {\r\n\tcomponents:{\r\n\t\tPopupTreeInput,\r\n\t\tKtTable,\r\n\t\tKtButton,\r\n\t\tTableColumnFilterDialog\r\n\t},\r\n\tdata() {\r\n\t\treturn {\r\n      size: 'small',\r\n      filters: {\r\n        id: ''\r\n      },\r\n      columns: [],\r\n      filterColumns: [],\r\n      pageRequest: {pageNum: 1, pageSize: 50},\r\n      pageResult: {},\r\n\r\n      operation: false, // true:新增, false:编辑\r\n      dialogVisible: false, // 新增编辑界面是否显示\r\n      editLoading: false,\r\n      dataFormRules: {\r\n        minnum: [\r\n          {required: true, message: '请输入起始人数', trigger: 'blur'},\r\n          { type: 'number', 
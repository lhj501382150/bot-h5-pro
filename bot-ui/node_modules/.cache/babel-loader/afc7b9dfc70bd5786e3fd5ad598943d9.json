AA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,YAAA,IAAA,GAAA,KAAA,KAAA,CAAA,CAAA,CAAA;AACA,QAAA,GAAA,CAAA,SAAA,GAAA,IAAA,CAAA,KAAA;AACA,YAAA,CAAA,GAAA,QAAA,CAAA,IAAA,CAAA,MAAA,CAAA,GAAA,GAAA;AACA,YAAA,CAAA,GAAA,IAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA,CAAA,MAAA,CAAA,GAAA,GAAA,CAAA;AACA,QAAA,GAAA,CAAA,QAAA,CAAA,CAAA,EAAA,CAAA,EAAA,CAAA,EAAA,CAAA;AACA;AACA,KAnBA;AAoBA,IAAA,MApBA,kBAoBA,CApBA,EAoBA;AACA,WAAA,CAAA,GAAA,CAAA,CAAA,OAAA,GAAA,GAAA;AACA,WAAA,CAAA,GAAA,CAAA,CAAA,CAAA,OAAA,GAAA,GAAA,IAAA,CAAA,CAAA;AACA,UAAA,IAAA,GAAA,IAAA;AACA,UAAA,GAAA,GAAA,KAAA,KAAA,CAAA,MAAA,CAAA,UAAA,IAAA,EAAA;AACA,eAAA,IAAA,CAAA,MAAA,IAAA,IAAA,CAAA,CAAA,IAAA,IAAA,CAAA,MAAA,IAAA,IAAA,CAAA,CAAA;AACA,OAFA,CAAA;;AAGA,UAAA,GAAA,IAAA,GAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,YAAA,IAAA,GAAA,GAAA,CAAA,CAAA,CAAA;AACA,aAAA,GAAA,GAAA,IAAA,CAAA,GAAA;AACA,OAHA,MAGA;AACA,aAAA,GAAA,GAAA,IAAA;AACA;;AAEA,UAAA,IAAA,CAAA,CAAA,GAAA,GAAA,IAAA,IAAA,CAAA,CAAA,GAAA,CAAA,GAAA,IAAA,IAAA,CAAA,CAAA,GAAA,GAAA,IAAA,IAAA,CAAA,CAAA,GAAA,CAAA,GAAA,EAAA;AACA,aAAA,MAAA,GAAA,KAAA;AACA,OAFA,MAEA;AACA,aAAA,MAAA,GAAA,IAAA;AACA,YAAA,CAAA,GAAA,CAAA,CAAA,OAAA,GAAA,EAAA;AACA,YAAA,CAAA,GAAA,CAAA,CAAA,OAAA,GAAA,EAAA;;AACA,YAAA,IAAA,CAAA,CAAA,GAAA,CAAA,EAAA,EAAA;AACA,UAAA,CAAA,IAAA,GAAA;AACA;;AACA,YAAA,IAAA,CAAA,CAAA,GAAA,EAAA,EAAA;AACA,UAAA,CAAA,IAAA,EAAA;AACA;;AACA,aAAA,GAAA,GAAA;AACA,UAAA,QAAA,EAAA,UADA;AAEA,UAAA,GAAA,EAAA,CAAA,GAAA,IAFA;AAGA,UAAA,IAAA,EAAA,CAAA,GAAA;AAHA,SAAA;AAKA;AAEA;AArDA;AA3BA,CAAA","sourcesContent":["<!--  -->\r\n<template>\r\n  <div>\r\n    <canvas id=\"tutorial\" width=\"360\" height=\"360\" class=\"item_block\" @mousemove=\"showXY($event)\"></canvas>\r\n    <p class=\"\" v-if=\"X  || Y \">坐标X:{{X}}  坐标Y:{{Y}}  {{mem}}</p>\r\n    <div class=\"tips\" :style=\"pos\" v-show=\"IS_TIP\">\r\n      X: {{X}} <br/>\r\n      Y: {{Y}} <br/>\r\n      {{mem}}\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name:'LandinfoModel',\r\n  props: {\r\n    items:{\r\n      type:Array,\r\n      default:[]\r\n    },\r\n    one:{}\r\n  },\r\n  data () {\r\n    return {\r\n      X:'',\r\n      Y:'',\r\n      mem:'',\r\n      pos:{},\r\n      IS_TIP: false\r\n    };\r\n  },\r\n\r\n  components: {},\r\n\r\n  computed: {},\r\n\r\n  mounted(){\r\n    this.draw()\r\n  },\r\n\r\n  methods: {\r\n    draw(){\r\n      var canvas = document.getElementById('tutorial');\r\n      var ctx = canvas.getContext('2d');\r\n      // ctx.clearRect(0, 0, canvas.width, canvas.height)\r\n      let color = this.one.color\r\n      ctx.fillStyle = color;\r\n      for(var i = 0;i< 360;i++){\r\n        for( var j = 0; j < 360;j++){\r\n          ctx.fillRect (i, j, 1, 1);\r\n        }\r\n      }\r\n      for(var i=0;i<this.items.length;i++){\r\n        var item = this.items[i]\r\n        ctx.fillStyle = item.color;\r\n        var x = parseInt(item.coordx) + 180;\r\n        var y = Math.abs(parseInt(item.coordy) - 180);\r\n        ctx.fillRect (x, y, 1, 1);\r\n      }\r\n    },\r\n    showXY(e){\r\n      this.X = e.offsetX - 180;\r\n      this.Y = (e.offsetY - 180) * -1;\r\n      const that = this\r\n      let res = this.items.filter(function(item){\r\n          return item.coordx == that.X && item.coordy ==that.Y;\r\n      })\r\n      if(res && res.length>0){\r\n        let item = res[0]\r\n        this.mem = item.mem\r\n      }else{\r\n        this.mem = '普通'\r\n      }\r\n\r\n      if(that.X > 180 || that.X < -180 || that.Y > 180 || that.Y < -180){\r\n        this.IS_TIP = false\r\n      }else{\r\n        this.IS_TIP = true\r\n        var x = e.offsetX + 20;\r\n        var y = e.offsetY + 20;\r\n        if(that.X < -90 ){\r\n          x += 100;\r\n        }\r\n        if(that.Y > 90 ){\r\n          y += 80;\r\n        }\r\n        this.pos = {\r\n          position:'absolute',\r\n          top: y + 'px',\r\n          left: x + 'px'\r\n        }\r\n      }\r\n\r\n    }\r\n  }\r\n}\r\n\r\n</script>\r\n<style scoped>\r\n  .item_block{\r\n    border: solid 1px #e2e2e2;\r\n    margin: 0 auto;\r\n    box-shadow: -10px -10px 50px 5px #e2e2e2;\r\n  }\r\n  .tips{\r\n    width: 80px;\r\n    height: 50px;\r\n    background-color: rgb(245, 249, 252);\r\n    opacity: 0.6;\r\n    border-radius: 15%;\r\n    padding:5px;\r\n    display: float;\r\n  }\r\n</style>\r\n"],"sourceRoot":"src/views/Trade/Compents"}]}
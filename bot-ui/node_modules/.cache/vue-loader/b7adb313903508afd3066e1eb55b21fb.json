","sourceRoot":"src/views/Sys","sourcesContent":["<template>\r\n  <div class=\"page-container\">\r\n\t<!--工具栏-->\r\n\t<div class=\"toolbar\" style=\"float:left;padding-top:10px;padding-left:15px;\">\r\n\t\t<el-form :inline=\"true\" :model=\"filters\" :size=\"size\">\r\n\t\t\t<el-form-item>\r\n\t\t\t\t<el-input v-model=\"filters.loginno\" placeholder=\"登陆编号\"></el-input>\r\n\t\t\t</el-form-item>\r\n      <el-form-item>\r\n        <el-input v-model=\"filters.loginname\" placeholder=\"用户名称\"></el-input>\r\n      </el-form-item>\r\n\t\t\t<el-form-item>\r\n\t\t\t\t<kt-button icon=\"fa fa-search\" :label=\"$t('action.search')\" perms=\"sys:user:view\" type=\"primary\" @click=\"findPage(null)\"/>\r\n\t\t\t</el-form-item>\r\n\t\t\t<el-form-item>\r\n\t\t\t\t<kt-button icon=\"fa fa-plus\" :label=\"$t('action.add')\" perms=\"sys:user:add\" type=\"primary\" @click=\"handleAdd\" />\r\n\t\t\t</el-form-item>\r\n\t\t</el-form>\r\n\t</div>\r\n\t<div class=\"toolbar\" style=\"float:right;padding-top:10px;padding-right:15px;\">\r\n\t\t<el-form :inline=\"true\" :size=\"size\">\r\n\t\t\t<el-form-item>\r\n\t\t\t\t<el-button-group>\r\n\t\t\t\t<el-tooltip content=\"刷新\" placement=\"top\">\r\n\t\t\t\t\t<el-button icon=\"fa fa-refresh\" @click=\"findPage(null)\"></el-button>\r\n\t\t\t\t</el-tooltip>\r\n\t\t\t\t<el-tooltip content=\"列显示\" placement=\"top\">\r\n\t\t\t\t\t<el-button icon=\"fa fa-filter\" @click=\"displayFilterColumnsDialog\"></el-button>\r\n\t\t\t\t</el-tooltip>\r\n\t\t\t\t<!--<el-tooltip content=\"导出\" placement=\"top\">\r\n\t\t\t\t\t<el-button icon=\"fa fa-file-excel-o\" @click=\"exportUserExcelFile\"></el-button>\r\n\t\t\t\t</el-tooltip>-->\r\n\t\t\t\t</el-button-group>\r\n\t\t\t</el-form-item>\r\n\t\t</el-form>\r\n\t\t<!--表格显示列界面-->\r\n\t\t<table-column-filter-dialog ref=\"tableColumnFilterDialog\" :columns=\"columns\"\r\n\t\t\t@handleFilterColumns=\"handleFilterColumns\">\r\n\t\t</table-column-filter-dialog>\r\n\t</div>\r\n\t<!--表格内容栏-->\r\n\t<kt-table permsEdit=\"sys:user